@using Newtonsoft.Json
@using WebApplication.Models

@model ImportViewModel

@{
    ViewData["Title"] = "Excel extensions";
    string exceptions = JsonConvert.SerializeObject(Model.Exceptions);
}


<script>
    const jsonParseExceptions = JSON.parse('@Html.Raw(exceptions)');
    window.jsonParseExceptions = jsonParseExceptions;

</script>
<div id="main"></div>
<div id="modal"></div>